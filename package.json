{
  "name": "Ajrasakha-Hackathon",
  "version": "1.0.0",
  "description": "Ajrasakha Hackathon project with frontend and server",
  "main": "index.js",
  "scripts": {
    "install:frontend": "cd frontend && pnpm install",
    "install:server": "cd server && pnpm install",
    "install": "pnpm run install:frontend && pnpm run install:server",
    "dev:frontend": "cd frontend && pnpm dev",
    "dev:server": "cd server && pnpm dev",
    "dev": "concurrently \"pnpm run dev:frontend\" \"pnpm run dev:server\"",
    "build:frontend": "cd frontend && pnpm build",
    "build:server": "cd server && pnpm build",
    "build": "pnpm run build:frontend && pnpm run build:server",
    "test:frontend": "cd frontend && pnpm test",
    "test:frontend:watch": "cd frontend && pnpm test:watch",
    "test": "pnpm run test:frontend",
    "start:server": "cd server && pnpm start",
    "prod": "pnpm run build && pnpm run start:server"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.28.2",
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "zod": "^4.3.6"
  }
}
