{
  "name": "ajrasakha-hackathon",
  "version": "1.0.0",
  "description": "Ajrasakha Hackathon project with client and server",
  "main": "index.js",
  "scripts": {
    "install:client": "cd client && pnpm install",
    "install:server": "cd server && pnpm install",
    "install": "pnpm run install:client && pnpm run install:server",
    "dev:client": "cd client && pnpm dev",
    "dev:server": "cd server && pnpm dev",
    "dev": "concurrently \"pnpm run dev:client\" \"pnpm run dev:server\"",
    "build:client": "cd client && pnpm build",
    "build:server": "cd server && pnpm build",
    "build": "pnpm run build:client && pnpm run build:server",
    "start:server": "cd server && pnpm start",
    "prod": "pnpm run build && pnpm run start:server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.28.2",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
